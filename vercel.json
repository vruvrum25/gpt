{
  "version": 2,
  "builds": [
    {
      "src": "*.{html,js,css,json,txt}",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/metrics/agent",
      "dest": "/api/metrics/agent.js"
    },
    {
      "src": "/metrics/identification$",
      "dest": "/api/metrics/identification.js"
    },
    {
      "src": "/metrics/identification/(.*)",
      "dest": "/api/metrics/identification/[...segments].js"
    },
    {
      "src": "/fp.js",
      "dest": "/fp.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
